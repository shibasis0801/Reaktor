{
  "name": "mehmaan-react",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "newFile": "zx scripts/newFile.mjs",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "clean": "powershell.exe Remove-Item -Recurse -Force $Env:TEMP\\metro-* && powershell.exe Remove-Item -Recurse -Force $Env:TEMP\\haste-map-*",
    "droidStart": "adb shell am start -n app.mehmaan.android/.MainActivity",
    "droidStop": "adb shell am force-stop app.mehmaan.android",
    "droidRestart": "yarn droidStop && yarn droidStart",
    "droidInstall": "adb install -r app/build/outputs/apk/debug/app-arm64-v8a-debug.apk",
    "droidBuild": "cd app && ./gradlew clean assembleDebug && cd ..",
    "droidRun": "yarn droidBuild && yarn droidInstall && yarn droidStart",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "podInstall": "cd ios && pod install"
  },
  "dependencies": {
    "@mehmaan/core": "*",
    "@mehmaan/feed": "*",
    "metro-config": "^0.77.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
